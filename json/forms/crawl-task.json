            "data-select-type": "static",
            "data-changed-callback" : "console.log",
            "required": "true"
        },
        "frame_id": {
            "type": "cubeselect", 
            "text": "Nguồn", 
            "action": "App\\Repositories\\Crawl\\FrameRepository::getSelectOption",
            "param":[true],
            "option_label_type": "label",
