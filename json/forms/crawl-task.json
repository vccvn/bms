        "quantity": { "type": "number", "text": "Số bài viết", "placeholder": "Nhập số bài viết", "required":"true" },
        "repeat_time": {
            "type": "cubeselect", 
            "text": "Thời gian lâp", 
            "option_label_type": "label",
