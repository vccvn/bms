            "data-changed-callback" : "console.log",
            "required": "true",
            "data":{
                "00:05:00": "5 phút",
                "00:10:00": "10 phút",
                "00:15:00": "15 phút",
                "00:30:00": "30 phút",
                "01:00:00": "1 tiếng",
                "02:00:00": "2 tiếng",
                "03:00:00": "3 tiếng",
                "06:00:00": "6 tiếng",
                "12:00:00": "12 tiếng",
                "24:00:00": "24 tiếng"
            }
        },
        "crawl_time": { 
            "type": "text", 
            "text": "Thời gian crawl", 
            "placeholder": "Nhập số bài viết", 
            "required":"true", 
            "data-format":"H:m:s", 
            "className":"datetime-picker",
            "default":"01:30:00"
        },
        "run_time": { "type": "checkbox", "text": "Thời gian crawl", "check_label":"Chọn"},
        "status": { 
            "type": "cubeselect", 
            "text": "Trạng thái", 
             "required":"true",
             "data":{
                 "1": "Bật",
                 "0": "Tắt"
             }
        },
        "cate_id": {
            "type": "cubeselect", 
            "text": "Danh mục", 
