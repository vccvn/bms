            "option_label_type": "label",
            "data-select-type": "static",
            "data-changed-callback" : "console.log",
            "required": "true"
        },
        "user_id": {
            "type": "cubeselect", 
            "text": "Tác giả", 
            "action": "App\\Repositories\\Users\\UserRepository::getAuthorSelectOption",
            "param":[true],
